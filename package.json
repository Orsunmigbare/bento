    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-react-hooks": "1.x",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "ora": "^3.0.0",
    "plop": "^2.0.0",
    "prettier": "^1.13.7",
    "react-testing-library": "^5.4.4",
