{
  "name": "@staccx/bento",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "test": "react-scripts test --coverage --watchAll=false --verbose  --testMatch **/packages/**/*.test.js --coverage",
    "styleguide": "cd packages/base && styleguidist server",
    "start": "cd packages/internal/styleguide && yarn start",
    "soy": "node packages/internal/soy",
    "release": "node ./packages/internal/release-bento/index.js",
    "clean": "rm -rf node_modules && rm yarn.lock && lerna clean --yes && lerna bootstrap && lerna run build",
    "buildAll": "lerna run prepare",
    "testAll": "lerna run prepublishOnly",
    "verify": "yarn buildAll && yarn testAll",
    "storybook:genreq": "node stories/generateRequireStories",
    "storybook": "yarn storybook:genreq && start-storybook -p 9009",
    "build-storybook": "yarn storybook:genreq && build-storybook -o ./.storybook/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stacc-flow/bento.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/stacc-flow/bento#readme",
  "description": "",
