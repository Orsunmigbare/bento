othing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  cursor: pointer;\n`\n\nconst Button = styled.a`\n  ${ButtonStyle};\n  &:hover,\n  &:focus {\n    ${Cursor} {\n      fill: ${p => p.theme.color.primary};\n    }\n  }\n`\n\nexport default LinkListItem\n"},"lineCount":null}},"hash":"73e265793948e22cf47d6748d58e407c","cacheData":{"env":{}}}