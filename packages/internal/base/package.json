  },
  "dependencies": {
    "@staccx/animations": "^7.1.1",
    "@staccx/formatting": "^7.1.1",
    "@staccx/math": "^7.1.1",
    "@staccx/soy": "^7.1.1",
    "@staccx/theme": "^7.1.1",
    "@staccx/utils": "^7.1.1",
    "babel-plugin-prismjs": "^1.0.2"
  }
