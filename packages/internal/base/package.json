  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stacc-flow/bento.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "react-scripts test --env=jsdom",
    "build": "yarn prettify && node build.js && rollup -c",
