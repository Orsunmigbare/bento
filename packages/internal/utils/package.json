{
  "name": "@staccx/utils",
  "version": "9.0.0",
  "description": "Utilites",
  "author": "StaccX",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stacc-flow/bento.git"
  },
  "main": "dist/export.js",
  "scripts": {
    "test": "react-scripts test --coverage --watchAll=false --env=node",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rm -rf dist && babel --root-mode upward src -d dist",
    "build:watch": "nodemon --watch src/ --exec \"babel --root-mode upward src -d dist\"",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "prettify": "prettier --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://stacc.jfrog.io/stacc/api/npm/npm/"
  },
  "devDependencies": {
    "colors": "^1.3.2",
    "glob": "^7.1.3",
    "progress": "^2.0.0"
  }
}
