{
  "name": "@staccx/property-value",
  "version": "8.84.0",
  "description": "Widget for showing property value based on person nationalId (Norway only)",
  "private": true,
  "author": "StaccX",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stacc-flow/bento.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom --coverage --passWithNoTests",
    "build:app": "react-scripts build",
    "eject": "react-scripts eject",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "build": "yarn prettify && rollup -c",
    "start:module": "rollup -c -w",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "prettify": "prettier --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://stacc.jfrog.io/stacc/api/npm/npm/"
  },
  "dependencies": {
    "@google/maps": "^0.4.6",
    "@staccx/base": "^7.15.0",
    "@staccx/formatting": "^8.84.0",
    "axios": "^0.18.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "recompose": "^0.27.1"
  },
  "devDependencies": {
    "@staccx/rollup-config": "^8.58.0",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewire-styled-components": "^3.0.2"
  },
  "peerDependencies": {
    "styled-components": "^4.0.2"
  }
}
