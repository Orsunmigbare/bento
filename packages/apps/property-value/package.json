{
  "name": "@staccx/property-value",
  "version": "6.0.0",
  "description": "Widget for showing property value based on person nationalId (Norway only)",
  "private": true,
  "author": "StaccX",
  "license": "UNLICENCED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/stacc-flow/bento.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom --coverage",
    "build:app": "react-scripts build",
    "eject": "react-scripts eject",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "build": "yarn prettify && rollup -c",
    "start:module": "rollup -c -w",
    "prepublish": "yarn test && yarn build",
    "prettify": "prettier --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://stacc.jfrog.io/stacc/api/npm/npm/"
  },
  "dependencies": {
    "@staccx/base": "^6.0.0",
    "@staccx/formatting": "^6.0.0",
    "@staccx/soy": "^6.0.0",
    "@staccx/theme": "^6.0.0"
  }
}
