{
  "name": "stacc-web",
  "alias": ["stac.cx"],
  "type": "npm",
  "static": {
    "public": "build",
    "redirects": [
      {
        "source": "/core",
        "destination": "/products/core/features",
        "type": 302
      },
      {
        "source": "/eng/customers",
        "destination": "/clients",
        "type": 302
      },
      {
        "source": "/about",
        "destination": "/team",
        "type": 302
      },
      {
        "source": "/get-in-touch-with-us",
        "destination": "/contact",
        "type": 302
      },
      {
        "source": "/news/:id",
        "destination": "/",
        "type": 302
      },
      {
        "source": "/jobs",
        "destination": "workable.stacc.com",
        "type": 302
      },
      {
        "source": "/news",
        "destination": "/",
        "type": 302
      },
      {
        "source": "/eng",
        "destination": "/",
        "type": 302
      },
      {
        "source": "/solutions",
        "destination": "/products/core/use-cases",
        "type": 302
      },
      {
        "source": "/flow",
        "destination": "/products/flow-process/features",
        "type": 302
      },
      {
        "source": "/stacc-flow",
        "destination": "/products/flow-process/features",
        "type": 302
      },
      {
        "source": "/privacypolicy",
        "destination": "/privacy-and-cookie-policy",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "manifest.json",
        "destination": "manifest.json"
      },
      {
        "source": "robots.txt",
        "destination": "robots.txt"
      },
      {
        "source": "sitemap.xml",
        "destination": "sitemap.xml"
      },
      {
        "source": ":file.:type",
        "destination": ":file.:type"
      },
      {
        "source": "images/:folder/:file",
        "destination": "images/:folder/:file"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "env": {
    "NODE_ENV": "production"
  },
  "files": ["build"]
}
